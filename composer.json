{
  "name": "compolomus/iniobject",
  "description": "Ini object",
  "license": "MIT",
  "keywords": [
    "ini config"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Koenig",
      "email": "compolom@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Compolomus\\IniObject\\": "src/"
    }
  },
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "suggest": {
    "ext-xdebug": "^2.7.0"
  },
  "scripts": {
    "test-coverage": "phpunit --coverage-clover clover.xml",
    "check": [
      "@cs-check"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  },
  "minimum-stability": "stable"
}
